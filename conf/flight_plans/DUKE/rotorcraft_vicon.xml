<!DOCTYPE flight_plan SYSTEM "flight_plan.dtd">



<!-- 

$ make clean && make

 -->

<flight_plan alt="1.5" ground_alt="0" lat0="36.005417" lon0="-78.940984" max_dist_from_home="20" name="Rotorcraft Vicon (Duke)" security_height="0.3">
  <header>
#include "autopilot.h"


</header>





  <waypoints>
    <waypoint name="HOME" x="0.0" y="0.0"/>
    <waypoint name="CLIMB" x="1.2" y="-0.6"/>
    <waypoint name="GOAL" x="2.0" y="2.0"/>
    <waypoint name="STDBY" x="-0.7" y="-0.8"/>
    <waypoint name="TD" x="0.8" y="-1.7"/>

    <waypoint name="HERE" x="1" y="1"/>
<!--     <waypoint lat="51.990630" lon="4.376823" name="p1"/>
    <waypoint lat="51.990614" lon="4.376779" name="p2"/>
    <waypoint lat="51.990636" lon="4.376759" name="p3"/>
    <waypoint lat="51.990651" lon="4.376805" name="p4"/> -->
    <!-- <waypoint lat="51.9906213" lon="4.3768628" name="FA1"/>
    <waypoint lat="51.9905874" lon="4.3767766" name="FA2"/>
    <waypoint lat="51.9906409" lon="4.3767226" name="FA3"/>
    <waypoint lat="51.9906737" lon="4.3768074" name="FA4"/> -->
    <waypoint name="FA1" x="-2.5242" y="3.5815"/>
    <waypoint name="FA2" x="2.5242" y="3.5815"/>
    <waypoint name="FA3" x="2.5242" y="-3.5815"/>
    <waypoint name="FA4" x="-2.5242" y="-3.5815"/>

    <waypoint name="demo1" x="-1.5" y="2.5"/>
    <waypoint name="demo2" x="1.5" y="-2.5"/>
    <waypoint name="demo3" x="-1.3" y="-2.3"/>
    <waypoint name="demo4" x="1.3" y="2.3"/> 

    <waypoint name="Room_Origin" x="0" y="0"/>

    <waypoint name="L_f" x="-1.579" y="3.170"  height="1.180"/>
    <waypoint name="L_c" x="-1.579" y="0.670"  height="1.180"/>
    <waypoint name="L_b" x="-1.579" y="-1.830" height="1.180"/>

    <waypoint name="R_f" x="1.587"  y="3.138"  height="1.225"/>
    <waypoint name="R_c" x="1.587"  y="0.638"  height="1.225"/>
    <waypoint name="R_b" x="1.587"  y="-1.862" height="1.225"/>

    <waypoint name="F_t" x="0.086"  y="4.091"  height="2.179"/>
    <waypoint name="F_c" x="0.086"  y="4.091"  height="1.179"/>
    <waypoint name="F_b" x="0.086"  y="4.091"  height="0.579"/>

    <waypoint name="B_t"     x="-0.089" y="-2.324" height="2.483"/>
    <waypoint name="B_c"     x="-0.089" y="-2.324" height="1.483"/>
    <waypoint name="B_b_pre" x="-0.089" y="-1.324" height="0.683"/>
    <waypoint name="B_b"     x="-0.089" y="-2.324" height="0.683"/>

    <waypoint name="T_t" x="2.559" y="-2.953" height="1.894"/>
    <waypoint name="T_c" x="2.559" y="-2.953" height="0.894"/>

    <waypoint name="B_swoop" x="-0.089" y="-1.324" height="0.683"/>

    <waypoint name="Center_Offset" x="1" y="0"/>


  </waypoints>
  <sectors>
    <sector color="red" name="Flight_Area">
      <corner name="FA1"/>
      <corner name="FA2"/>
      <corner name="FA3"/>
      <corner name="FA4"/>
    </sector>
  </sectors>
  <variables>
    <variable var="LeftCenterX" init="-1.579" type="float"/>
    <variable var="LeftCenterZ" init="1.180"  type="float"/>
    <variable var="RightCenterX" init="1.587" type="float"/>
    <variable var="RightCenterZ" init="1.225"  type="float"/>
    <variable var="ErrorWindow" init="0.15"  type="float"/>
  </variables>
  <blocks>
    <block name="Wait GPS">
      <call fun="NavKillThrottle()"/>
      <while cond="!GpsFixValid()"/>
    </block>
    <block name="Holding point">
      <call fun="NavKillThrottle()"/>
      <attitude pitch="0" roll="0" throttle="0" until="FALSE" vmode="throttle"/>
    </block>
    <block name="Start Engine">
      <call fun="NavResurrect()"/>
      <attitude pitch="0" roll="0" throttle="0" until="FALSE" vmode="throttle"/>
    </block>
    <block name="Takeoff" strip_button="Takeoff" strip_icon="takeoff.png">
      <exception cond="stateGetPositionEnu_f()->z > 1.0" deroute="Standby"/>
      <call fun="NavSetWaypointHere(WP_STDBY)"/>
      <stay climb="nav_climb_vspeed" vmode="climb" wp="Room_Origin"/>
    </block>
    <block name="Standby" strip_button="Standby" strip_icon="home.png">
      <stay wp="Room_Origin"/>
    </block>
    <block name="START" strip_button="Go" strip_icon="lookfore.png">
      <call fun="NavSetWaypointHere(WP_GOAL)"/>
    </block>
    <block name="StayGoal">
      <stay wp="GOAL"/>
    </block>
<!--     <block name="stay_p1">
      <stay wp="p1"/>
    </block>
    <block name="go_p2">
      <call fun="nav_set_heading_deg(90)"/>
      <go wp="p2"/>
      <deroute block="stay_p1"/>
    </block> -->
<!--     <block name="line_p1_p2">
      <go from="p1" hmode="route" wp="p2"/>
      <stay until="stage_time>10" wp="p2"/>
      <go from="p2" hmode="route" wp="p1"/>
      <deroute block="stay_p1"/>
    </block>
    <block name="route">
      <go from="p1" hmode="route" wp="p3"/>
      <go from="p3" hmode="route" wp="p4"/>
      <go from="p4" hmode="route" wp="p1"/>
      <deroute block="stay_p1"/>
    </block> -->
<!--     <block name="Oval">
      <oval p1="p1" p2="p2" radius="-1"/>
    </block> -->
    <block name="epic demo">
        <call fun="NavSetWaypointHere(WP_HERE)"/>
        <call fun="nav_set_heading_towards_waypoint(WP_demo1)"/>
        <go from="HERE" hmode="route" wp="demo1"/>
        <call fun="nav_set_heading_towards_waypoint(WP_demo2)"/>
        <go from="demo1" hmode="route" wp="demo2"/>
        <call fun="nav_set_heading_towards_waypoint(WP_demo3)"/>
        <go from="demo2" hmode="route" wp="demo3"/>
        <call fun="nav_set_heading_towards_waypoint(WP_demo4)"/>
        <go from="demo3" hmode="route" wp="demo4"/>
        <call fun="nav_set_heading_towards_waypoint(WP_STDBY)"/>
        <go from="demo4" hmode="route" wp="HOME"/>
        <deroute block="land here"/>
    </block>

    <block name="TB - L_f">
      <stay wp="L_f"/>
    </block>
    <block name="TB - L_c">
      <stay wp="L_c"/>
    </block>
    <block name="TB - L_b">
      <stay wp="L_b"/>
    </block>

    <block name="TB - R_f">
      <stay wp="R_f"/>
    </block>
    <block name="TB - R_c">
      <stay wp="R_c"/>
    </block>
    <block name="TB - R_b">
      <stay wp="R_b"/>
    </block>

    <block name="TB - F_t">
      <stay wp="F_t"/>
    </block>
    <block name="TB - F_c">
      <stay wp="F_c"/>
    </block>
    <block name="TB - F_b">
      <stay wp="F_b"/>
    </block>

    <block name="TB - B_t">
      <stay wp="B_t"/>
    </block>
    <block name="TB - B_c">
      <stay wp="B_c"/>
    </block>
    <block name="TB - B_b_pre">
      <stay wp="B_b_pre"/>
    </block>
    <block name="TB - B_b">
      <stay wp="B_b"/>
    </block>

    <block name="TB - T_t">
      <stay wp="T_t"/>
    </block>
    <block name="TB - T_c">
      <stay wp="T_c"/>
    </block>


    <block name="And Test">
      
      <call fun="nav_set_heading_towards_waypoint(WP_L_b)"/>
      <go wp="L_b" />

      <call fun="nav_set_heading_towards_waypoint(WP_L_f)"/>
      <while cond=" ! And(  ( ErrorWindow > fabs(GetPosX()-LeftCenterX) ), ( ErrorWindow > fabs(GetPosAlt()-LeftCenterZ) )  )  " />

      <go wp="L_f" exceeding_time="4"/>
      
      <call fun="nav_set_heading_towards_waypoint(WP_R_b)"/>
      <go wp="R_b" />
      
      <call fun="nav_set_heading_towards_waypoint(WP_R_f)"/>
      <while cond=" ! And(  ( ErrorWindow > fabs(GetPosX()-RightCenterX) ), ( ErrorWindow > fabs(GetPosAlt()-RightCenterZ) )  )  " />

      <go wp="R_f" exceeding_time="4"/>
      
      <call fun="nav_set_heading_towards_waypoint(WP_Room_Origin)"/>
      <go wp="Room_Origin" />
      
      <deroute block="land here"/>
    </block>

    <block name="And Test Part 0">
      <exception cond="stateGetPositionEnu_f()->z > 1.0" deroute="And Test Part 0a"/>
      <stay climb="nav_climb_vspeed" vmode="climb" wp="Room_Origin"/>
    </block>

    <block name="And Test Part 0a">
      <go wp="Room_Origin" />
      <while cond="LessThan(NavBlockTime(),6)" /> 
      <deroute block="And Test Part 1 - Go To L_b"/>
    </block>

    <block name="And Test Part 1 - Go To L_b">
      <exception cond="MoreThan(NavBlockTime(),3.5)" deroute="And Test Part 2"/>
      <call fun="nav_set_heading_towards_waypoint(WP_L_b)"/>
      <go wp="L_b" />
    </block>

    <block name="And Test Part 2">
      <!-- <exception cond="MoreThan(NavBlockTime(),3)" deroute="And Test Part 3"/> -->
      <call fun="nav_set_heading_towards_waypoint(WP_L_f)"/>
      <while cond=" ! And(  ( ErrorWindow > fabs(GetPosX()-LeftCenterX) ), ( ErrorWindow > fabs(GetPosAlt()-LeftCenterZ) )  )  " />
    </block>

    <block name="And Test Part 3">
      <exception cond="MoreThan(NavBlockTime(),5)" deroute="And Test Part 4 - Go To R_b"/>
      <go wp="L_f" exceeding_time="4"/>
    </block>

    <block name="And Test Part 4 - Go To R_b">
      <exception cond="MoreThan(NavBlockTime(),6)" deroute="And Test Part 4a"/>
      <call fun="nav_set_heading_towards_waypoint(WP_R_b)"/>
      <go wp="R_b" />
    </block>

    <block name="And Test Part 4a">
      <exception cond="MoreThan(NavBlockTime(),15)" deroute="And Test Part 5"/>
      <call fun="nav_set_heading_towards_waypoint(WP_R_f)"/>
    </block>

    <block name="And Test Part 5">
      <!-- <exception cond="MoreThan(NavBlockTime(),15)" deroute="And Test Part 6"/> -->
      <while cond=" ! And(  ( ErrorWindow > fabs(GetPosX()-RightCenterX) ), ( ErrorWindow > fabs(GetPosAlt()-RightCenterZ) )  )  " />
    </block>

    <block name="And Test Part 6">
      <exception cond="MoreThan(NavBlockTime(),8)" deroute="And Test Part 7 - Go To RoomOrigin"/>
      <go wp="R_f" exceeding_time="4"/>
    </block>

    <block name="And Test Part 7 - Go To RoomOrigin">
      <exception cond="MoreThan(NavBlockTime(),2)" deroute="And Test Part 8 - Circle Around"/>
      <call fun="nav_set_heading_towards_waypoint(WP_Room_Origin)"/>
      <go wp="Center_Offset" />
    </block>



<!-- Next Block:  -->
    

    <block name="And Test Part 8 - Circle Around">      
      <deroute block="And Test Part 9 - Go To L_b"/>
    </block>

    <block name="And Test Part 9 - Go To L_b">
      <exception cond="MoreThan(NavBlockTime(),5)" deroute="And Test Part 10"/>
      <call fun="nav_set_heading_towards_waypoint(WP_L_b)"/>
      <go wp="L_b" />
    </block>

    <block name="And Test Part 10">
      <!-- <exception cond="MoreThan(NavBlockTime(),3)" deroute="And Test Part 11"/> -->
      <call fun="nav_set_heading_towards_waypoint(WP_L_f)"/>
      <while cond=" ! And(  ( ErrorWindow > fabs(GetPosX()-LeftCenterX) ), ( ErrorWindow > fabs(GetPosAlt()-LeftCenterZ) )  )  " />
    </block>

    <block name="And Test Part 11">
      <exception cond="MoreThan(NavBlockTime(),5)" deroute="And Test Part 12 - Go To F_t"/>
      <go wp="L_f"/>
    </block>

    <block name="And Test Part 12 - Go To F_t">
      <exception cond="MoreThan(NavBlockTime(),6)" deroute="And Test Part 13 - Go to B_swoop"/>
      <go wp="F_t"/>
    </block>

    <block name="And Test Part 13 - Go to B_swoop">
      <exception cond="MoreThan(NavBlockTime(),6)" deroute="And Test Part 14 - Go to R_b"/>
      <go wp="B_swoop"/>
    </block>

    <block name="And Test Part 14 - Go to R_b">
      <exception cond="MoreThan(NavBlockTime(),1.5)" deroute="And Test Part 14a"/>
      <call fun="nav_set_heading_towards_waypoint(WP_R_b)"/>
      <go wp="R_b" />
    </block>

    <block name="And Test Part 14a">
      <exception cond="MoreThan(NavBlockTime(),15)" deroute="And Test Part 15"/>
      <call fun="nav_set_heading_towards_waypoint(WP_R_f)"/>
    </block>

    <block name="And Test Part 15">
      <!-- <exception cond="MoreThan(NavBlockTime(),15)" deroute="And Test Part 16"/> -->
      <while cond=" ! And(  ( ErrorWindow > fabs(GetPosX()-RightCenterX) ), ( ErrorWindow > fabs(GetPosAlt()-RightCenterZ) )  )  " />
    </block>

    <block name="And Test Part 16">
      <exception cond="MoreThan(NavBlockTime(),8)" deroute="And Test Part 17 - Go To RoomOrigin"/>
      <go wp="R_f" exceeding_time="4"/>
    </block>

    <block name="And Test Part 17 - Go To RoomOrigin">
      <exception cond="MoreThan(NavBlockTime(),6)" deroute="And Test Part 18"/>
      <call fun="nav_set_heading_towards_waypoint(WP_Center_Offset)"/>
      <go wp="Center_Offset" />
<!--       <while cond=" ! And(  ( ErrorWindow > fabs(GetPosX()-0) ), ( ErrorWindow > fabs(GetPosY()-0) )  )  " /> -->
    </block>





    <block name="And Test Part 18">      
      <deroute block="land here"/>
    </block>


    <!-- 
    <block name="And Result Test">
    </block> -->



    <block name="land here" strip_button="Land Here" strip_icon="land-right.png">
      <call fun="NavSetWaypointHere(WP_TD)"/>
    </block>
    <block name="land">
      <go wp="TD"/>
    </block>
    <block name="flare">
      <exception cond="NavDetectGround()" deroute="Holding point"/>
      <exception cond="!nav_is_in_flight()" deroute="landed"/>
      <call fun="NavStartDetectGround()"/>
      <stay climb="nav_descend_vspeed" vmode="climb" wp="TD"/>
    </block>
    <block name="landed">
      <call fun="NavKillThrottle()"/>
      <attitude pitch="0" roll="0" throttle="0" until="FALSE" vmode="throttle"/>
    </block>
  </blocks>
</flight_plan>

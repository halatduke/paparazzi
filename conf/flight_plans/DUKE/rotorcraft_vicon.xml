<!DOCTYPE flight_plan SYSTEM "flight_plan.dtd">

<flight_plan alt="1.2" ground_alt="-0.3000888" lat0="36.005417" lon0="-78.940984" max_dist_from_home="100" name="Rotorcraft Vicon (Duke)" security_height="0.3">
  <header>
#include "autopilot.h"
  </header>

    <block name ="START">
    </block>

    <block name="Next Waypoint!">
    </block>

    <block name="Flying">
	<go wp = "NEXT" />
    </block>

    <block name="Arrived at Waypoint">
	<stay wp = "NEXT" />
    </block>

    <block name="PAUSE" >
	<call fun="NavSetWaypointHere(WP_STDBY)"/>
	<stay wp="STDBY"/>
    </block>

    <!-- <block name="TB - L_f">
      <stay wp="L_f"/>
    </block>

    <block name="TB - L_c">
      <stay wp="L_c"/>
    </block>

    <block name="TB - L_b">
      <stay wp="L_b"/>
    </block>

    <block name="TB - R_f">
      <stay wp="R_f"/>
    </block>

    <block name="TB - R_c">
      <stay wp="R_c"/>
    </block>

    <block name="TB - R_b">
      <stay wp="R_b"/>
    </block> 



    <block name="TB - F_RED_t">
      <stay wp="F_RED_t"/>
    </block>
    <block name="TB - F_RED_c">
      <stay wp="F_RED_c"/>
    </block>
